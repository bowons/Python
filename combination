import itertools

def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num ** 0.5) + 1):
        if num % i == 0:
            return False
    return True

def solution(nums):
    # 가능한 모든 조합을 구하기 위해 itertools.combinations 사용
    combinations = itertools.combinations(nums, 3)
    count = 0
    for comb in combinations:
        total = sum(comb)
        if is_prime(total):
            count += 1
    return count
